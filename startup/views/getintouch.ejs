	    <section class="contact p-5 topmarg">
    <div class="container-fluid">
      <div class="row">
        <div class="col-lg-5 pb-4">
          <h3 class="display-4 mb-5 text-white">Get In Touch</h3>
           <form action="/contactus" method="POST">
            <div class="form-group py-4">
              <input type="text" class="form-control my-2 p-2 input" placeholder="Name" name="contact[Name]">
              <label for="name" class="label">Name</label>
            </div>
            <div class="form-group py-4">
              <input type="email" class="form-control my-2 p-2 input" placeholder="Email Address" name="contact[Mail]">
              <label for="email" class="label">Email Address</label>
            </div>
			  <div class="form-group py-4">
              <input type="text" class="form-control my-2 p-2 input" placeholder="Department" name="contact[Department]">
              <label for="name" class="label">Department</label>
            </div>
			  <div class="form-group py-4">
                              <input type="text" maxlength="10" pattern="\d{10}" class="form-control my-2 p-2 input" name="contact[Phone]" placeholder="Phone Number"                   required>
                              <label for="number" class="label">Phone Number</label>
                            </div>
			  <div class="form-group py-4">
                  <textarea type="text" class="form-control p-2 input" name="contact[Description]" placeholder="Description/Name of the Book" required></textarea>
                              <label for="desc" class="label my-1">Description</label>
                            </div>
                            <div class="form-group py-4 my-2">
                              <label class="text-white">We'll contact you soon after you submit!</label>
                            </div>
                            <button type="submit" class="btn btn-block p-2 font-weight-bold text-uppercase submit-button">Submit!</button>
                          </form>
                        </div>
                      </div>
                    </div>
            
       
  </section>
